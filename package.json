{
  "name": "nextly-template",
  "version": "2.0.0",
  "description": "Nextly Landing Page Template",
  "private": true,
  "scripts": {
    "predev": "npm run fix:blog-metadata",
    "dev": "next dev",
    "fix:blog-metadata": "node scripts/fix-missing-metadata.js",
    "validate:blog-metadata": "node scripts/validate-blog-metadata.js",
    "build:blog": "npm run fix:blog-metadata && npm run validate:blog-metadata && node scripts/build-blog.js",
    "build": "npm run build:blog && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/web3templates/nextly-template"
  },
  "keywords": [
    "nextjs",
    "tailwindcss",
    "free"
  ],
  "author": "Surjith S M",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "^2.0.3",
    "@heroicons/react": "^2.1.3",
    "@next/mdx": "^14.2.3",
    "next": "^14.2.35",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.4"
  },
  "devDependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "^14.2.35",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
